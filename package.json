{
  "name": "svelte-awesome-icons",
  "version": "0.2.5",
  "description": "Font Awesome SVG Icon components for Svelte",
  "author": {
    "name": "Shinichi Okada",
    "email": "connect@codewithshin.com",
    "url": "https://blog.codewithshin.com"
  },
  "bugs": "https://github.com/shinokada/svelte-awesome-icons/issues",
  "homepage": "https://github.com/shinokada/svelte-awesome-icons",
  "license": "MIT",
  "scripts": {
    "dev": "vite dev",
    "build": "svelte-kit sync && svelte-package",
    "package": "svelte-package",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "package:publish": "standard-version && git push --follow-tags origin main && npm run build && npm publish ./package"
  },
  "devDependencies": {
    "@playwright/test": "^1.26.0",
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/kit": "next",
    "@sveltejs/package": "next",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.1",
    "svelte": "^3.50.1",
    "svelte-check": "^2.9.1",
    "svelte-preprocess": "^4.10.7",
    "svelte2tsx": "^0.5.19",
    "tslib": "^2.4.0",
    "typescript": "^4.8.3",
    "vite": "^4.0.1"
  },
  "type": "module",
  "keywords": [
    "svelte",
    "sveltekit",
    "font awesome",
    "svg icons",
    "tailwindcss",
    "icons"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/shinokada/svelte-awesome-icons"
  }
}